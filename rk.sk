    else if {_Chestplate} is 3:
        set {_p}'s chestplate to diamond chestplate of protection 1
    else if {_Chestplate} is 4:
        set {_p}'s chestplate to diamond chestplate of protection 2
    else if {_Chestplate} is 5:
        set {_p}'s chestplate to diamond chestplate of protection 2 and blast protection 3
    else if {_Chestplate} is 6:
        set {_p}'s chestplate to gold chestplate of protection 4 and unbreaking 1
    else if {_Chestplate} is 7:
