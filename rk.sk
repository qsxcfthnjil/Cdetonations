        set {_p}'s helmet to iron helmet of protection 1 and unbreaking 1 and blast protection 2
    else if {_Helmet} is 2:
        set {_p}'s helmet to iron helmet of protection 2 and unbreaking 1 and blast protection 3
    else if {_Helmet} is 3:
        set {_p}'s helmet to diamond helmet of protection 1
    else if {_Helmet} is 4:
        set {_p}'s helmet to diamond helmet of protection 2
    else if {_Helmet} is 5:
        set {_p}'s helmet to diamond helmet of protection 2 and blast protection 3
    else if {_Helmet} is 6:
        set {_p}'s helmet to gold helmet of protection 4 and unbreaking 1
